{
    "schemaVersion": 1,
    "name": "Dynamic View Distance",
    "id": "ehdynview",
    "version": "${version}",
    "icon": "assets/ehdynview/icon.png",
    "description": "A minecraft mod that makes the view distance dynamic to save resources",
    "authors": [
        "Einholz"
    ],
    "contact": {
        "homepage": "https://www.curseforge.com/members/albert_einholz/projects",
        "sources": "https://github.com/Albert-Einholz/Dynamic-View-Distance",
        "issues": "https://github.com/Albert-Einholz/Dynamic-View-Distance/issues"
    },
    "license": "Apache-2.0",
