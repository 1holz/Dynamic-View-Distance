    "entrypoints": {
        "main": [
            "de.einholz.ehdynview.DynView"
        ],
        "client": [
            "de.einholz.ehdynview.DynView"
        ],
        "server": [
            "de.einholz.ehdynview.DynView"
        ],
        "modmenu": [
